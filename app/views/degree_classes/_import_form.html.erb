<%= form_tag(import_year_degree_classes_path(@year), :multipart => true, :class => "form-horizontal") do %>
  <h3>
    Import Degree Classes
  </h3>

  <div class='control-group'>
    <div class='controls'>
      <%= file_field_tag "file" %>
    </div>
  </div>

  <div class='form-actions'>
    <%= submit_tag "Upload", :class => 'btn btn-primary' %>
  </div>

<% end %>
