<tr class="lab-hours">
  <td>
    Lab hours
  </td>
  <% @year.terms.each do |t| %>
    <% if @course.taught_in_term?(t) %>
      <% t.weeks.each do |w| %>
        <td>
          <%= text_field_tag "course_weeks[labs][#{t.no}][#{w.no}]", find_existing_hours(@course.course_weeks, w, nil, "Lab"), :class => "tiny term-#{t.no} lab", :id => "#{t.no}-#{w.no}-lab" %>
        </td>
      <% end %>
      <td></td>
    <% end %>
  <% end %>
</tr>
